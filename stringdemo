package string.java;

import java.util.Vector;
import static java.lang.System.out;
public class Stringdemo {

	public static void main(String[] args) {
		int age;
		Vector v;
		String s="Hello";
		out.println("Object id:"+s.hashCode());
		String s1="Hello";
		out.println("Object id:"+s1.hashCode());
		out.println(s1);
		String name="Sachin";
		name.concat("Tendulkar");
		out.println(name);
		StringBuffer myname=new StringBuffer("kalyan");
		myname.append(" pandu");
		out.println(myname);
		StringBuilder sb=new StringBuilder("Hindusthan");
		sb.append(" Computers Limited");
			out.println("is working in "+sb);
			v=new Vector();
			v.addElement(s1);
			v.addElement(myname);
			System.out.println("Collection v:"+v);
		// TODO Auto-generated method stub

	}

}
